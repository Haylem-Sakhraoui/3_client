<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<fx:root fx:id="captcha" alignment="CENTER" stylesheets="@style.css" type="VBox" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <Label fx:id="captchaQuestion" text="Label" />
    <TextField id="captchaQuestion" fx:id="captchaInput" prefWidth="200.0" promptText="Solution" />
    <Button fx:id="okButton" mnemonicParsing="false" onAction="#okButtonPressed" text="OK" />
  </children>
</fx:root>
