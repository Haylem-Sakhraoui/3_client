<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="637.0" prefWidth="548.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.BackPubController">
    <children>
      <Button fx:id="btn_post" layoutX="454.0" layoutY="603.0" mnemonicParsing="false" onAction="#Add" prefHeight="17.0" prefWidth="78.0" text="Post" />
      <TableView fx:id="tableView_Publication" layoutX="104.0" layoutY="42.0" prefHeight="292.0" prefWidth="431.0">
        <columns>
          <TableColumn fx:id="Col_Texte" onEditCommit="#changeTextCellEvent" prefWidth="219.0" resizable="false" text="Texte" />
            <TableColumn fx:id="Col_likes" minWidth="0.0" prefWidth="72.0" text="Likes" />
            <TableColumn fx:id="Col_date" prefWidth="135.0" text="Date" />
        </columns>
      </TableView>
      <TextArea fx:id="text_area" layoutX="13.0" layoutY="530.0" prefHeight="63.0" prefWidth="522.0" />
      <TableView fx:id="tableView_Links" layoutX="13.0" layoutY="341.0" prefHeight="117.0" prefWidth="522.0">
        <columns>
          <TableColumn fx:id="Col_Links" prefWidth="480.0" text="Click on Post to See Photos Linked to it :" />
        </columns>
      </TableView>
      <Label layoutX="235.0" layoutY="504.0" text="Publication" />
      <TextField fx:id="tf_ph" layoutX="52.0" layoutY="603.0" prefHeight="25.0" prefWidth="393.0" />
      <Label layoutX="15.0" layoutY="607.0" text="Links:" />
      <TextField fx:id="tf_id" layoutX="58.0" layoutY="500.0" prefHeight="25.0" prefWidth="30.0" />
      <Label layoutX="11.0" layoutY="503.0" text="ID_user" />
      <Button fx:id="btn_fe" layoutX="80.0" layoutY="465.0" mnemonicParsing="false" onAction="#load_FrontEnd" prefHeight="25.0" prefWidth="85.0" text="Normal View" />
      <Button fx:id="btn_refresh" layoutX="11.0" layoutY="465.0" mnemonicParsing="false" onAction="#Display" prefHeight="25.0" prefWidth="62.0" text="Refresh" />
      <Button fx:id="btn_addpic" disable="true" layoutX="457.0" layoutY="465.0" mnemonicParsing="false" onAction="#add_pic" text="Add Picture" />
      <TextField fx:id="tf_url" disable="true" layoutX="175.0" layoutY="465.0" prefHeight="25.0" prefWidth="269.0" />
      <TextField fx:id="tf_search" layoutX="75.0" layoutY="11.0" onKeyTyped="#refresh" prefHeight="25.0" prefWidth="461.0" />
      <Button fx:id="btn_search" layoutX="17.0" layoutY="11.0" mnemonicParsing="false" onAction="#search" text="Search" />
      <Label layoutX="28.0" layoutY="46.0" text="Username" />
   </children>
